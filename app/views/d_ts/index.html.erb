<p id="notice"><%= notice %></p>

<h1>Listing Dts</h1>

<table>
  <thead>
    <tr>
      <th>Document name</th>
      <th>Version</th>
      <th>Go live date</th>
      <th>Transfer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dts.each do |dt| %>
      <tr>
        <td><%= dt.document_name %></td>
        <td><%= dt.version %></td>
        <td><%= dt.go_live_date %></td>
        <td><%= dt.transfer_id %></td>
        <td><%= link_to 'Show', dt %></td>
        <td><%= link_to 'Edit', edit_dt_path(dt) %></td>
        <td><%= link_to 'Destroy', dt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dts', new_dt_path %>
